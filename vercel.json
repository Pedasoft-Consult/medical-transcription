{
  "version": 2,
  "builds": [
    {
      "src": "run.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "17mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "run.py"
    }
  ],
  "env": {
    "FLASK_APP": "run.py",
    "VERCEL": "1",
    "FLASK_ENV": "production",
    "FLASK_DEBUG": "False",
    "PORT": "2000",
    "SQLALCHEMY_POOL_SIZE": "5",
    "SQLALCHEMY_MAX_OVERFLOW": "10",
    "SQLALCHEMY_POOL_RECYCLE": "300"
  },
  "regions": ["iad1"]
}